{
  "expo": {
    "name": "horizontalScroll",
    "slug": "horizontal-scroll",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "androidStatusBar": {
      "backgroundColor": "#0ABFBC"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Implementing Horizontal Scrolling in React Native",
    "githubUrl": "https://github.com/Pravash2/horizontal_scroll_react_native.git"
  }
}
